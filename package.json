	"scripts": {
		"test": "npx hardhat test",
		"compile": "npx hardhat compile --force",
		"flatten": "npx hardhat flatten > flattened/DeHubPricePrediction.sol",
		"fork:testnet": "npx hardhat node --fork $(dotenv -p MORALIS_BSC_TESTNET_ARCHIVE_URL) --fork-block-number 12585457",
		"fork:mainnet": "npx hardhat node --fork $(dotenv -p MORALIS_BSC_MAINNET_ARCHIVE_URL) --fork-block-number 11134446",
		"console:local": "npx hardhat console --network localhost",
		"console:testnet": "npx hardhat console --network testnet",
		"deploy:testnet": "npx hardhat run scripts/deploy.ts --network testnet",
		"deploy:mainnet": "npx hardhat run scripts/deploy.ts --network mainnet",
		"upgrade:testnet": "npx hardhat run scripts/deploy-upgrade.ts --network testnet",
		"upgrade:mainnet": "npx hardhat run scripts/deploy-upgrade.ts --network mainnet"
	},
